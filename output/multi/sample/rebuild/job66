SELECT COUNT(*) FROM title t,movie_info mi,movie_keyword mk,movie_info_idx mi_idx,movie_companies mc WHERE t.id=mi_idx.movie_id AND t.id=mc.movie_id AND mk.movie_id=mc.movie_id AND t.id=mi_idx.movie_id AND t.production_year>2014 AND t.production_year<1995 AND mi.info_type_id>16 AND mi_idx.info_type_id!=113 AND mc.company_type_id=2;