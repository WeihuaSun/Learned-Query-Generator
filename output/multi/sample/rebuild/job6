SELECT COUNT(*) FROM movie_info mi,movie_keyword mk,movie_info_idx mi_idx,movie_companies mc,cast_info ci WHERE mi.movie_id=ci.movie_id AND mk.movie_id=mi_idx.movie_id AND mi.movie_id=mk.movie_id AND mi_idx.movie_id=mc.movie_id AND mi.info_type_id=16 AND mi_idx.info_type_id=101 AND mc.company_id=22956 AND ci.role_id>4 AND ci.role_id<4;