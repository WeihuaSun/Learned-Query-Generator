SELECT COUNT(*) FROM title t,movie_info mi,movie_keyword mk,movie_companies mc,cast_info ci WHERE mk.movie_id=ci.movie_id AND t.id=mi.movie_id AND t.id=mk.movie_id AND mc.movie_id=ci.movie_id AND t.production_year>2014 AND t.production_year<2014 AND mi.info_type_id=16 AND mc.company_type_id=2 AND ci.role_id>1 AND ci.role_id<4;