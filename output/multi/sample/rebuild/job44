SELECT COUNT(*) FROM title t,movie_info_idx mi_idx,movie_companies mc,cast_info ci WHERE t.id=mc.movie_id AND mi_idx.movie_id=ci.movie_id AND mi_idx.movie_id=mc.movie_id AND t.production_year>1995 AND t.production_year<1995 AND mi_idx.info_type_id=101 AND mc.company_type_id!=2 AND ci.role_id>4 AND ci.role_id<4;