SELECT COUNT(*) FROM title t,movie_keyword mk,movie_info_idx mi_idx,movie_companies mc,cast_info ci WHERE mk.movie_id=mi_idx.movie_id AND mi_idx.movie_id=ci.movie_id AND t.id=mc.movie_id AND mk.movie_id=mi_idx.movie_id AND t.production_year>=2014 AND t.production_year<2014 AND mi_idx.info_type_id=101 AND mc.company_type_id=2 AND ci.role_id>4 AND ci.role_id<4;