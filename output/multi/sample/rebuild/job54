SELECT COUNT(*) FROM movie_info mi,movie_keyword mk,movie_info_idx mi_idx,movie_companies mc,cast_info ci WHERE mi.movie_id=mc.movie_id AND mk.movie_id=ci.movie_id AND mc.movie_id=ci.movie_id AND mi.movie_id=mk.movie_id AND mi.info_type_id=16 AND mi_idx.info_type_id=100 AND mc.company_type_id=2 AND ci.role_id>4 AND ci.role_id<1;