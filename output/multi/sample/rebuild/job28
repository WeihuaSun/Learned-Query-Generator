SELECT COUNT(*) FROM title t,movie_info mi,movie_info_idx mi_idx,movie_companies mc,cast_info ci WHERE mi.movie_id=ci.movie_id AND t.id=mi.movie_id AND mi.movie_id=mi_idx.movie_id AND t.id=mc.movie_id AND t.production_year>1995 AND t.production_year<2014 AND mi.info_type_id=16 AND mi_idx.info_type_id=101 AND ci.role_id=7;